        object Memo1: TMemo
          Left = 520
          Top = 102
          Width = 465
          Height = 131
          Color = clWhite
          Font.Charset = DEFAULT_CHARSET
          Font.Color = clBlack
          Font.Height = -12
          Font.Name = 'MS Sans Serif'
          Font.Style = []
          Lines.Strings = (
            'For each time window we have to decide if pitch is'
            'harmonic or '#39'pure tone'#39'. We compute two pitch estimate'
            'harmonic and mean-frequency. Harmonic pitch is usually '
            'lower than pure tones: if harmonic pitch estimate is'
            'higher than threshold, we chose mean-frequency as '
            'pitch estimate. Also, we go for harmonic pitch estimate'
            'only if goodness of pitch is high enough or if the Wiener'
            'entropy is low enough. Otherwise, mean-frequency is '
            'the pitch estimate.')
          ParentFont = False
          ReadOnly = True
          TabOrder = 5
        end
        object pitch_averaging: TRadioGroup
