        object Memo8: TMemo
          Left = 520
          Top = 238
          Width = 465
          Height = 131
          Color = clWhite
          Font.Charset = DEFAULT_CHARSET
          Font.Color = clBlack
          Font.Height = -12
          Font.Name = 'MS Sans Serif'
          Font.Style = []
          Lines.Strings = (
            'One common problem with pitch calculation is that'
            'the pitch estimate are not always robust, e.g., when'
            'some sounds are tonal and other sounds are noisy, '
            'even within a syllable. The solution we offer is taking'
            'the goodness of pitch into acount while computing the'
            'mean pitch of the syllable, such that part of the syllable '
            'that has a better goodness of pitch will have a higher '
            'weight. This procedure can often provide a much more'
            'stable pitch estimate. ')
          ParentFont = False
          ReadOnly = True
          TabOrder = 7
        end
