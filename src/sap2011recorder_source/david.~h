// Structures and objects:
        struct Training_Session
        {
          bool enabled;
          bool state;   // on or off (dynamic)
          int start_hour;
          int start_min;
          int end_hour;
          int end_min;
          bool random_onset; // or start session on time
          int random_onset_delay; // number of Timer (30sec) cycles to wait before starting the session
          int playback_list; // 0 -- list 1; 1 -- list 2; 2 -- key dependent key1-list 1 key2 -- list2
          bool enable_quota; // or unlimited quota
          int quota; // the limit for number of playbacks if enable_quota is true
          int song_played;
          float gap; // ISI
          int gap_timer;
          int operant; // false=passive ; 1=key1; 2=key2; 3=key1&key2
          float odds;  // relevant if passive -- odds of playback in each second
        };
        Training_Session Session[20];



